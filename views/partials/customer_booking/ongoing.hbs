<div class="container">
   <div class="row">
   {{#each packageInfo}} 
      {{#if this.completed}} 
      {{else}} 
      <div class="col-md-3">
         <div class="pricing-item entermediate">
            <div class="pi-top">
                  <h4>{{this.program}}</h4>
            </div>
            <div class="pi-price" style="margin-bottom:0;">
                  <span><b>Rs.</b><h1>{{this.price}}</h1></span>
            </div >
            <ul>
               <li class="mr-3"><b>For sessions plan:</b>{{this.session}}</li>
               <li class="mr-3"><b>Validity For: </b>{{this.validity}}</li>
            </ul> 
         </div>
         <br>
         <h3>Progress</h3>
         <div class="w3-light-grey w3-border w3-border-grey w3-round-xlarge">
            <div class="w3-round-xlarge w3-teal" style="height:24px;width:{{this.progress}}%;">
               {{this.progress}}%</div>
          </div>
      </div>
      {{/if}}
   {{/each}}   
   </div>
   <br>
   <br>
</div>